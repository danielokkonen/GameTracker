// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Game {
  id Int @id @default(autoincrement())
  name String 
  franchise String
  start String?
  end String?
  summary String?
  developer String?
  publisher String?
  genres String?
  platforms String?
  coverImage String?
  created String
  updated String?
}

model Tokens {
  service String @id
  token String
  type String
  expires_at Int
}
